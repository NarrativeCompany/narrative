{
  "name": "@narrative/web",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost"
    }
  },
  "dependencies": {
    "@narrative/shared": "0.1.0",
    "antd": "^3.15.2",
    "apollo-link-state": "^0.4.2",
    "deepmerge": "^3.2.0",
    "es-cookie": "^1.2.0",
    "formik": "^1.5.1",
    "froala-editor": "^2.9.4",
    "jwt-decode": "^2.2.0",
    "loadable-components": "^2.2.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-froala-wysiwyg": "^2.9.4-1",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-recaptcha": "^2.3.10",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "3.1.0",
    "react-share": "^2.4.0",
    "react-visibility-sensor": "^5.0.2",
    "react-youtube": "^7.9.0",
    "recompose": "^0.30.0",
    "styled-components": "^3.5.0-0",
    "typeface-lato": "^0.0.54"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "generate-sw": "sw-precache --root=build --config scripts/sw-precache-config.js && uglifyjs build/service-worker.js -o build/service-worker.js",
    "build-snap": "yarn run build && react-snap && yarn run generate-sw",
    "translate:compile": "tsc -p .  --target ES6 --module esnext --jsx preserve --outDir extracted",
    "translate:extract": "babel  \"extracted/**/*.jsx\"",
    "translate:manage": "node scripts/translationRunner.js",
    "translate:build": "yarn run translate:compile && yarn run translate:extract && yarn run translate:manage && npx rimraf extracted && npx rimraf src/shared/i18n/extracted"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/jwt-decode": "^2.2.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^11.12.0",
    "@types/prop-types": "^15.7.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.10",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "@types/react-intl": "^2.3.17",
    "@types/react-recaptcha": "^2.3.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-share": "^2.1.1",
    "@types/react-visibility-sensor": "^5.0.1",
    "@types/react-youtube": "^7.6.1",
    "@types/recompose": "^0.30.5",
    "@types/uuid": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "customize-cra": "^0.2.12",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.6.2",
    "react-intl-translations-manager": "^5.0.3",
    "react-snap": "^1.23.0",
    "sw-precache": "^5.2.1",
    "ts-import-plugin": "^1.5.5",
    "typescript": "~3.1.6",
    "typescript-plugin-styled-components": "^1.4.2"
  },
  "react-snap": {
    "cacheAjaxRequests": true,
    "inlineCss": true,
    "http2PushManifest": true,
    "include": [
      "/200.html"
    ]
  }
}
